{
  "manifest_version": 3,
  "name": "Pro Workspace Manager",
  "version": "5.0.0",
  "author": "Panagiotis Gousis",
  "developer": {
    "name": "Panagiotis Gousis",
    "url": "https://www.homeforme.gr/"
  },
  "homepage_url": "https://github.com/panosss/homeforme",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage",
    "sessions",
    "tabHide",
    "menus",
    "contextualIdentities",
    "unlimitedStorage",
    "downloads"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Workspaces Popup"
  },
  "sidebar_action": {
    "default_panel": "sidebar.html",
    "default_title": "Workspaces Sidebar"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "workspace-pro-ultimate@dev.com"
    }
  },
  "icons": {
    "16": "/icons/icon-16.svg",
    "32": "/icons/icon-32.svg",
    "48": "/icons/icon-48.svg",
    "96": "/icons/icon-96.svg"
  }
}
